{
  "name": "@zodash/doreamon",
  "version": "0.4.53",
  "description": "utils like a doreamon",
  "keywords": [
    "zodash",
    "compress",
    "image"
  ],
  "author": "zero <tobewhatwewant@gmail.com>",
  "homepage": "https://github.com/zcorky/zodash#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "roots": [
      "__tests__"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.(spec|test)\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": -10
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zcorky/zodash.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest -w 1",
    "coverage": "codecov"
  },
  "bugs": {
    "url": "https://github.com/zcorky/zodash/issues"
  },
  "dependencies": {
    "@zcorky/deep-copy": "^0.0.2",
    "@zcorky/deep-equal": "^0.0.1",
    "@zcorky/device": "^0.0.6",
    "@zcorky/fz": "^1.2.5",
    "@zcorky/is": "^1.0.7",
    "@zcorky/moment": "^1.1.0",
    "@zcorky/query-string": "^1.0.2",
    "@zcorky/storage": "^1.2.3",
    "@zodash/camel-case": "^0.1.11",
    "@zodash/capitalize": "^0.1.6",
    "@zodash/cookie": "^0.0.1",
    "@zodash/debounce": "^0.1.6",
    "@zodash/debug": "^0.0.14",
    "@zodash/delay": "^0.1.6",
    "@zodash/dom": "^0.1.8",
    "@zodash/error": "^0.1.7",
    "@zodash/event": "^0.1.7",
    "@zodash/find": "^0.1.7",
    "@zodash/format": "^1.2.7",
    "@zodash/fs": "^0.0.15",
    "@zodash/get": "^1.1.6",
    "@zodash/group-by": "^1.1.9",
    "@zodash/load-js": "^0.2.12",
    "@zodash/logger": "^0.2.16",
    "@zodash/memoize": "^1.1.6",
    "@zodash/ms": "^0.1.9",
    "@zodash/omit": "^1.1.6",
    "@zodash/once": "^0.1.5",
    "@zodash/pick": "^1.1.6",
    "@zodash/random": "^1.2.10",
    "@zodash/snake-case": "^0.1.8",
    "@zodash/throttle": "^0.1.6",
    "@zodash/unique": "^0.1.6",
    "@zodash/uuid": "^0.1.6"
  }
}
